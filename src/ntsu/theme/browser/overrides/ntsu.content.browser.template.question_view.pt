<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="ntsu.addon">
<body>


<metal:content-core fill-slot="content-core">
<metal:content-core define-macro="content-core"
                    tal:define="toc context/table_of_contents|nothing;">

  <!--Main Content Start-->
  <div class="cp-page-content">
    <!--Home Latest News Start-->
    <div class="cp-home-latest-news gap-25">
      <div class="container">
        <div class="row">
          <div class="col-md-12">

<h2 tal:content="context/title" />
<hr>
<h3>Answer</h3>
<p tal:repeat="item context/answer" tal:content="item">

<h3>Correct Answer</h3>
<strong tal:content="context/correctAns">



          </div>



        </div>
      </div>
    </div>
    <!--Home Latest News End-->
  </div>
  <!--Main Content End-->



</metal:content-core>
</metal:content-core>

</body>
</html>
